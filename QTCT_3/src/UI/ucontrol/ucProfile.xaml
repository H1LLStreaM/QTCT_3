        <Label x:Name="labMLV" HorizontalAlignment="Left" Margin="112,87.667,0,0" Grid.Row="2" VerticalAlignment="Top" Width="146" HorizontalContentAlignment="Right" Height="25"/>
        <Label Content="项目净利润:" HorizontalAlignment="Left" Margin="23,128.667,0,0" Grid.Row="2" VerticalAlignment="Top" RenderTransformOrigin="0.35,-0.316"/>
