        <Label x:Name="labRatio" HorizontalAlignment="Left" Margin="112,167.667,0,0" Grid.Row="2" VerticalAlignment="Top" Width="145" Height="25" HorizontalContentAlignment="Right"/>
        <Label Content="提成金额:" HorizontalAlignment="Left" Margin="35,212.667,0,0" Grid.Row="2" VerticalAlignment="Top"/>
        <Label x:Name="labTCJE" HorizontalAlignment="Left" Margin="112,212.667,0,0" Grid.Row="2" VerticalAlignment="Top" Width="144" HorizontalContentAlignment="Right" Height="25"/>

    </Grid>
</UserControl>
