        <Label Content="项目毛利润:" HorizontalAlignment="Left" Margin="21,50.667,0,0" Grid.Row="2" VerticalAlignment="Top" RenderTransformOrigin="0.565,-0.053"/>
        <Label x:Name="labMoney3" HorizontalAlignment="Left" Margin="112,50.667,0,0" Grid.Row="2" VerticalAlignment="Top" Width="146" Height="25" HorizontalContentAlignment="Right"/>
